{
    "name": "spinen/laravel-browser-filter",
    "description": "Filters http requests based on browser type.",
    "keywords": [
        "browser hawk",
        "laravel",
        "middleware",
        "spinen"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jimmy Puckett",
            "email": "jimmy.puckett@spinen.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/cache": "~5.5|~6",
        "illuminate/routing": "~5.5|~6",
        "illuminate/support": "~5.5|~6",
        "mobiledetect/mobiledetectlib": "~2.8",
        "ua-parser/uap-php": "~3.5"
    },
    "require-dev": {
        "illuminate/http": "~5.5|~6",
        "mockery/mockery": "^1",
        "phpunit/phpunit": "~7.0.1|~8.0",
        "psy/psysh": "^0.9.9",
        "symfony/thanks": "^1.1",
        "symfony/var-dumper": "~3.0|^4.2"
    },
    "autoload": {
        "files": [
            "src/Support/helper.php"
        ],
        "psr-4": {
            "Spinen\\BrowserFilter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spinen\\BrowserFilter\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spinen\\BrowserFilter\\FilterServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
