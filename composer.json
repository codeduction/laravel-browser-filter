{
    "name": "spinen/laravel-browser-filter",
    "description": "Filters request based on browser type.",
    "keywords": [
        "browser hawk",
        "laravel",
        "library",
        "middleware",
        "spinen"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jimmy Puckett",
            "email": "jimmy.puckett@spinen.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "illuminate/cache": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*",
        "illuminate/routing": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*",
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*",
        "mobiledetect/mobiledetectlib": "~2.8",
        "ua-parser/uap-php": "~3.4.1"
    },
    "require-dev": {
        "illuminate/http": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*",
        "mockery/mockery": "^1",
        "phpunit/phpunit": "~7.0",
        "psy/psysh": "^0.5.1",
        "satooshi/php-coveralls": "^1",
        "symfony/thanks": "^1.0",
        "symfony/var-dumper": "~3.0"
    },
    "autoload": {
        "files": [
            "src/Support/helper.php"
        ],
        "psr-4": {
            "Spinen\\BrowserFilter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spinen\\BrowserFilter\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spinen\\BrowserFilter\\FilterServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
